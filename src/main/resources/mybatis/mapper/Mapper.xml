    <delete id="deleteTravel">
        DELETE FROM travel WHERE travel_id=#{id}
    </delete>

    <delete id="deleteTrip">
        DELETE FROM trip WHERE trip_id=#{id}
    </delete>

    <delete id="deleteAccommodation">
        DELETE FROM accommodation WHERE accommodation_id=#{id}
    </delete>

    <delete id="deleteStay">
        DELETE FROM stay WHERE stay_id=#{id}
    </delete>



</mapper>